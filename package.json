{
	"name": "rainmaker-obsidian",
	"version": "3.0.6",
	"description": "Clean Letta chat interface for Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && npm run dist",
		"dist": "node -e \"const fs=require('fs');const dir='dist/rainmaker-obsidian';fs.mkdirSync(dir,{recursive:true});['main.js','manifest.json','styles.css'].forEach(f=>fs.copyFileSync(f,dir+'/'+f));console.log('Copied to '+dir)\"",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"letta",
		"ai",
		"agent"
	],
	"author": "PROJECT RAINMAKER",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"@letta-ai/letta-client": "^0.0.68660"
	}
}
